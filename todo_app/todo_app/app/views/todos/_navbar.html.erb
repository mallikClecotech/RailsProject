          <li class="nav-item">
            <%= link_to 'Sign out',destroy_user_session_path,method: :delete,class:"nav-link active btn btn-primary" %>
          </li>
          <%else%>
          <li class="nav-item">
            <%= link_to 'Sign up',new_user_registration_path,class:"nav-link active btn btn-primary"%>
